\n  data () {\n    return {\n      openGroupIndex: 0\n    }\n  },\n\n  created () {\n    this.refreshIndex()\n  },\n\n  watch: {\n    '$route' () {\n      this.refreshIndex()\n    }\n  },\n\n  methods: {\n    refreshIndex () {\n      const index = resolveOpenGroupIndex(\n        this.$route,\n        this.items[0].children\n      )\n      if (index > -1) {\n        this.openGroupIndex = index\n      }\n    },\n\n    toggleGroup (index) {\n      this.openGroupIndex = index === this.openGroupIndex ? -1 : index\n    },\n\n    isActive (page) {\n      return isActive(this.$route, page.regularPath)\n    }\n  }\n}\n\nfunction resolveOpenGroupIndex (route, items) {\n  for (let i = 0; i < items.length; i++) {\n    const item = items[i]\n    if (descendantIsActive(route, item)) {\n      return i\n    }\n  }\n  return -1\n}\n\nfunction descendantIsActive (route, item) {\n  if (item.type === 'group') {\n    return item.children.some(child => {\n      if (child.type === 'group') {\n        return descendantIsActive(route, child)\n      } else {\n        return child.type === 'page' && isActive(route, child.path)\n      }\n    })\n  }\n  return false\n}\n"],"mappings":"AACA,OAAOA,kBAAkB,MAAM,0CAA0C;AACzE,OAAOC,kBAAkB,MAAM,0CAA0C;AACzE,SAASC,QAAQ,QAAQ,SAAS;AAElC,eAAe;EACbC,IAAI,EAAE,gBAAgB;EAEtBC,UAAU,EAAE;IAAEJ,kBAAkB;IAAEC;EAAmB,CAAC;EAEtDI,KAAK,EAAE,CACL,OAAO,EACP,OAAO;EAAG;EACV,cAAc,CAAC;EAAA,CAChB;;EAEDC,IAAIA,CAAA,EAAI;IACN,OAAO;MACLC,cAAc,EAAE;IAClB,CAAC;EACH,CAAC;EAEDC,OAAOA,CAAA,EAAI;IACT,IAAI,CAACC,YAAY,EAAE;EACrB,CAAC;EAEDC,KAAK,EAAE;IACL,QAAQC,CAAA,EAAI;MACV,IAAI,CAACF,YAAY,EAAE;IACrB;EACF,CAAC;EAEDG,OAAO,EAAE;IACPH,YAAYA,CAAA,EAAI;MACd,MAAMI,KAAK,GAAGC,qBAAqB,CACjC,IAAI,CAACH,MAAM,EACX,IAAI,CAACI,KAAK,CAAC,CAAC,CAAC,CAACC,QAAQ,CACvB;MACD,IAAIH,KAAK,GAAG,CAAC,CAAC,EAAE;QACd,IAAI,CAACN,cAAc,GAAGM,KAAK;MAC7B;IACF,CAAC;IAEDI,WAAWA,CAAEJ,KAAK,EAAE;MAClB,IAAI,CAACN,cAAc,GAAGM,KAAK,KAAK,IAAI,CAACN,cAAc,GAAG,CAAC,CAAC,GAAGM,KAAK;IAClE,CAAC;IAEDX,QAAQA,CAAEgB,IAAI,EAAE;MACd,O